{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds serve",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": [
        "watch",
        "--with-mocks"
      ],
      "env": {
        "CDS_ENV": "hybrid"
      },
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}